import java.util.Scanner;

public class Exe_7 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tam = 10;
		int[] array = new int[tam];
		
		System.out.println("Coloque os 10 numeros: ");
		for(int i = 0; i >= tam; i++) {
			array[i] = sc.nextInt();
			System.out.println(array[i]);
		}
		
		sc.close();
		

	}

}
