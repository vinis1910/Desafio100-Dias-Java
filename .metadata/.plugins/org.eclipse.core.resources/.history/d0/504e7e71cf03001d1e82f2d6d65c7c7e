import java.util.Scanner;

public class Exe_7 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int tam = 10;
		int[] arrayA = new int[tam];
		int[] arrayB = new int[tam];
		
		System.out.println("Coloque os 10 numeros: ");
		for(int i = 0; i <= tam; i++) {
			arrayA[i] = sc.nextInt();
			arrayB[i] = arrayA[i];
		}
		
		System.out.println(arrayB[tam]);
		
		
		
		sc.close();
	}

}
